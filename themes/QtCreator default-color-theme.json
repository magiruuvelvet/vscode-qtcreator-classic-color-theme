    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#737373"
            }
        },
        {
            "name": "Comments: storage",
            "scope": "comment.block.preprocessor",
            "settings": {
                "fontStyle": "",
                "foreground": "#000080"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "name": "Comments: Documentation brief | param | return",
            "scope": [
                "storage.type.class.doxygen",
                "storage.type.class.jsdoc",
                "comment.line.keyword.yard"
            ],
            "settings": {
                "foreground": "#0000ff"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#660000"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#000000"
            }
